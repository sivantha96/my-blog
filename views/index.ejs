<%- include('partials/header') %>

<div class="ui main container">
    <% blogs.forEach(function(blog){ %>
        <div>
            <h1> <%= blog.title %> </h1>
            <img src="<%= blog.image %>">
            <span> <%= blog.created %> </span>
            <p> <%= blog.body %> </p>
        </div>
    <% }) %>
</div>



<%- include('partials/footer') %>